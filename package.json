{
  "name": "@haaretz/generator-interactive",
  "version": "0.0.0-development",
  "description": "Scaffold an Haaretz interactive page or element",
  "repository": "haaretz/generator-haaretz-generator-interactive",
  "author": "TxHawks",
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator"
  ],
  "scripts": {
    "gc": "git cz",
    "format": "eslint . --fix",
    "pretest": "eslint . --fix",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "doctoc": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^2.4.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "camel-case": "^4.1.1",
    "chalk": "^4.0.0",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "yeoman-generator": "^4.8.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "README.md": "doctoc --github",
    "*.{css,scss,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "license": "MIT"
}
